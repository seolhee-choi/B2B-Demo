<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.backend.mapper.TestMapper">

    <select id="getConnectCheck" resultType="com.example.backend.dto.TestDto">
        SELECT NAME,
               CONN
          FROM CONNECT_CHECK
         WHERE 1=1
    </select>

</mapper>
